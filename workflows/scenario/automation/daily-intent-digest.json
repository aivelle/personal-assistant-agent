{
  "workflow_id": "daily_intent_digest",
  "trigger": "scheduled_nightly",
  "actions": [
    {
      "type": "gather_user_intents",
      "period": "today",
      "output_var": "intent_summary"
    },
    {
      "type": "notion_create",
      "database_id": "idea_bank_001",
      "properties": {
        "Idea Title": "Daily Summary - {{today}}",
        "Idea": "{{intent_summary}}"
      }
    }
  ]
} 